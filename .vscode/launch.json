    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}/bin",
            "program": "${workspaceFolder}/cmd/testserver/main.go",
            "args": [
                "-o","${workspaceFolder}/bin/debug_bin"
            ],
            "windows": {
                "args": [
                    "-o","${workspaceFolder}/bin/debug_bin.exe"
                ],
            },
            "env": {
                "CGO_ENABLED": "0"
            }
        }
    ]
}